<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="3" parallel="classes" name="Suite">
	<test name="Test">
		<classes>
			<class
				name="com.Selenium.ValidateMouseOperationsParallelMethodsSeleniumGrid">
				<methods>
					<include name="ValidateDragAndDrop"></include>
				</methods>
			</class>

			<class
				name="com.Selenium.ValidateMouseOperationsParallelMethodsSeleniumGrid">
				<methods>
					<include name="ValidateRightCickOperation"></include>
				</methods>
			</class>

			<class
				name="com.Selenium.ValidateMouseOperationsParallelMethodsSeleniumGrid">
				<methods>
					<include name="ValidateDoubleClickOperation"></include>
				</methods>
			</class>
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
